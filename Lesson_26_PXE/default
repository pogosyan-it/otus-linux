default menu.c32
promt 0
timeout 300
ONTIME local
menu title  #### PXE BOOT Menu ####
              label 1
              menu label ^1) Install Centos 8.2 on Bios-based system
              kernel /vmlinuz
              append initrd=/initrd.img inst.repo=http://192.168.111.2/centos8-install devfs=nomount
              label 2
              menu label ^2) TEXT  Install Centos 8.2 on Bios-based system
              kernel /vmlinuz
              append initrd=/initrd.img inst.ks=http://192.168.111.2/centos8-install/ks.cfg
